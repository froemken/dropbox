{
  "name": "sfroemken/fal_dropbox",
  "type": "typo3-cms-extension",
  "description": "Add a FAL driver for Dropbox to TYPO3",
  "version": "2.0.0",
  "license": "GPL-2.0+",
  "keywords": ["typo3", "TYPO3 CMS", "dropbox", "fal", "fal_dropbox"],
  "homepage": "http://www.typo3lexikon.de",
  "authors": [
    {
      "name": "Stefan Froemken",
      "email": "froemken@gmail.com",
      "role": "Lead Developer"
    }
  ],
  "support": {
    "email": "froemken@gmail.com",
    "issues": "https://github.com/froemken/fal_dropbox/issues",
    "source": "https://github.com/froemken/fal_dropbox"
  },
  "require": {
    "php": ">=5.4.0 <8.0",
    "typo3/cms": ">=6.2.14,<9.0",
    "kunalvarma05/dropbox-php-sdk": ">=0.2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8.0"
  },
  "autoload": {
    "psr-4": {
      "SFroemken\\FalDropbox\\": "Classes",
      "Psr\\Http\\Message\\": "vendor/psr/http-message/src",
      "Kunnu\\Dropbox\\": "vendor/kunalvarma05/dropbox-php-sdk/src/Dropbox",
      "Illuminate\\": "vendor/tightenco/collect/src/Illuminate",
      "GuzzleHttp\\Psr7\\": "vendor/guzzlehttp/psr7/src",
      "GuzzleHttp\\Promise\\": "vendor/guzzlehttp/promises/src",
      "GuzzleHttp\\": "vendor/guzzlehttp/guzzle/src"
    }
  }
}
